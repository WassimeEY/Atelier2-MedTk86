<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <ApplicationIcon>IconeLogiciel.ico</ApplicationIcon>
    <Title>MediaTek86</Title>
    <PackageIcon>IconeLogiciel.png</PackageIcon>
    <StartupObject>MedTk86.Program</StartupObject>
    <Platforms>AnyCPU;x64;x86</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="IconeLogiciel.ico" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="dal\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\IconeLogiciel.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MySql.Data" Version="8.4.0" />
  </ItemGroup>

</Project>